{
  "name": "tagtime.js",
  "version": "0.3.0",
  "description": "A node.js implementation of [tagtime](https://github.com/dreeves/tagtime)",
  "main": "index.js",
  "scripts": {
    "test": "mocha -R spec",
    "start": "electron ./build/index.js",
    "build": "npm run build:be; npm run build:fe",
    "build:watch": "npm run build:be:watch & npm run build:fe:watch",
    "build:be": "babel -s es2015 -d build",
    "build:be:watch": "babel -s -w es2015 -d build",
    "build:fe": "babel -s assets/es2015 -d assets/js",
    "build:fe:watch": "babel -s -w assets/es2015 -d assets/js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sjmarshy/tagtime.js.git"
  },
  "keywords": [
    "tagtime",
    "node"
  ],
  "author": "Sam Marshall <sjmarshy@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sjmarshy/tagtime.js/issues"
  },
  "homepage": "https://github.com/sjmarshy/tagtime.js",
  "dependencies": {
    "coffee-script": "^1.7.1",
    "gulp": "^3.8.6",
    "hapi": "^6.2.1",
    "jsnox": "^2.1.2",
    "mersenne": "0.0.3",
    "moment": "^2.7.0",
    "npmlog": "^0.1.1",
    "q": "^1.0.1",
    "ramda": "^0.19.1",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "underscore": "^1.6.0",
    "zero-env": "^1.0.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "gulp-coffee": "^2.1.1"
  }
}
